    "# PYTHON CELL 1: Setup and Configuration\n",
    "import pandas as pd\n",
    "import json\n",
    "from snowflake.snowpark import Session\n",
    "from snowflake.snowpark.functions import col, to_geography, lit, concat\n",
    "from snowflake.snowpark.types import *\n",
    "import streamlit as st\n",
    "\n",
    "st.title(\"Texas gSSURGO Processing - Stage Upload Method\")\n",
    "st.write(\"Process gSSURGO data uploaded via Snowflake stages\")\n",
    "\n",
    "# Target table configuration\n",
    "TARGET_TABLES = {\n",
    "    'MUPOLYGON': 'SSURGO_MUPOLYGON',\n",
    "    'component': 'SSURGO_COMPONENT', \n",
    "    'mapunit': 'SSURGO_MAPUNIT',\n",
    "    'chorizon': 'SSURGO_CHORIZON',\n",
    "    'muaggatt': 'SSURGO_MUAGGATT'\n",
    "}\n",
    "\n",
    "st.write(\"**Upload Methods Available:**\")\n",
    "st.write(\"1. **Web UI Upload** (Recommended)\")\n",
    "st.write(\"2. **SnowSQL Command Line**\")\n",
    "st.write(\"3. **Direct Parquet Files**\")\n",
    "\n",
    "st.write(\"**Target Tables:**\")\n",
    "for source, target in TARGET_TABLES.items():\n",
    "    st.write(f\"• {source} → {target}\")"
   ]
