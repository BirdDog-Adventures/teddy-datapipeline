    "            ORDER BY avg_organic_matter DESC, avg_fertilizer_cost ASC\n",
    "            LIMIT 20\n",
    "            \"\"\"\n",
    "            \n",
    "            results = session.sql(ag_land_sql).to_pandas()\n",
