    "-- SQL CELL 6: Create Spatial Geometries and Indexes\n",
    "-- Update MUPOLYGON table with spatial geometries\n",
    "UPDATE SSURGO_MUPOLYGON \n",
    "SET GEOMETRY = TO_GEOGRAPHY(GEOMETRY_WKT)\n",
    "WHERE GEOMETRY_WKT IS NOT NULL AND GEOMETRY IS NULL;\n",
    "\n",
    "-- Add spatial index\n",
    "ALTER TABLE SSURGO_MUPOLYGON \n",
    "ADD SEARCH OPTIMIZATION ON EQUALITY(MUKEY, MUSYM, AREASYMBOL);\n",
    "\n",
    "-- Create performance indexes\n",
    "CREATE INDEX IF NOT EXISTS idx_mupolygon_mukey ON SSURGO_MUPOLYGON(MUKEY);\n",
    "CREATE INDEX IF NOT EXISTS idx_mupolygon_areasymbol ON SSURGO_MUPOLYGON(AREASYMBOL);\n",
    "CREATE INDEX IF NOT EXISTS idx_component_mukey ON SSURGO_COMPONENT(MUKEY);\n",
    "CREATE INDEX IF NOT EXISTS idx_component_cokey ON SSURGO_COMPONENT(COKEY);\n",
    "CREATE INDEX IF NOT EXISTS idx_component_majflag ON SSURGO_COMPONENT(MAJCOMPFLAG);\n",
    "CREATE INDEX IF NOT EXISTS idx_chorizon_cokey ON SSURGO_CHORIZON(COKEY);\n",
    "CREATE INDEX IF NOT EXISTS idx_chorizon_depth ON SSURGO_CHORIZON(HZDEPT_R);\n",
    "CREATE INDEX IF NOT EXISTS idx_mapunit_mukey ON SSURGO_MAPUNIT(MUKEY);\n",
    "CREATE INDEX IF NOT EXISTS idx_muaggatt_mukey ON SSURGO_MUAGGATT(MUKEY);"
