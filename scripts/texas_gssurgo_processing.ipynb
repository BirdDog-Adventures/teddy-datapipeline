    "-- SQL CELL 5: Create Nutrient Analysis Views\n",
    "-- NUTRIENT ESTIMATION VIEW\n",
    "CREATE OR REPLACE VIEW SOIL_NUTRIENTS AS\n",
    "SELECT \n",
    "    c.COKEY,\n",
    "    c.HZNAME,\n",
    "    c.HZDEPT_R,\n",
    "    c.HZDEPB_R,\n",
    "    \n",
    "    -- pH Category (affects all nutrient availability)\n",
    "    c.PH1TO1H2O_R as SOIL_PH,\n",
    "    CASE \n",
    "        WHEN c.PH1TO1H2O_R < 5.5 THEN 'Very Acidic'\n",
    "        WHEN c.PH1TO1H2O_R < 6.0 THEN 'Acidic'  \n",
    "        WHEN c.PH1TO1H2O_R < 6.8 THEN 'Slightly Acidic'\n",
    "        WHEN c.PH1TO1H2O_R < 7.3 THEN 'Neutral'\n",
    "        WHEN c.PH1TO1H2O_R < 8.0 THEN 'Slightly Alkaline'\n",
    "        ELSE 'Alkaline'\n",
    "    END as PH_CATEGORY,\n",
    "    \n",
    "    -- Phosphorus (most commonly measured nutrient in SSURGO)\n",
    "    c.PBRAY1_R as PHOSPHORUS_BRAY1_PPM,\n",
    "    CASE \n",
    "        WHEN c.PBRAY1_R IS NULL THEN 'No Data'\n",
    "        WHEN c.PBRAY1_R < 10 THEN 'Very Low'\n",
    "        WHEN c.PBRAY1_R < 20 THEN 'Low'\n",
    "        WHEN c.PBRAY1_R < 30 THEN 'Medium'\n",
    "        WHEN c.PBRAY1_R < 50 THEN 'High'\n",
    "        ELSE 'Very High'\n",
    "    END as PHOSPHORUS_LEVEL,\n",
    "    \n",
    "    -- Nitrogen (estimated from organic matter)\n",
    "    c.OM_R as ORGANIC_MATTER_PCT,\n",
    "    ROUND(c.OM_R * 1000 * 0.05, 0) as ESTIMATED_NITROGEN_PPM,\n",
    "    CASE \n",
    "        WHEN c.OM_R IS NULL THEN 'No Data'\n",
    "        WHEN c.OM_R < 1.0 THEN 'Very Low'\n",
    "        WHEN c.OM_R < 2.0 THEN 'Low'\n",
    "        WHEN c.OM_R < 3.0 THEN 'Medium'\n",
    "        WHEN c.OM_R < 4.0 THEN 'High' \n",
    "        ELSE 'Very High'\n",
    "    END as NITROGEN_POTENTIAL,\n",
    "    \n",
    "    -- Potassium (estimated from CEC and base saturation)\n",
    "    c.CEC7_R as CEC_MEQ_100G,\n",
    "    c.SUMBASES_R as BASE_SATURATION_PCT,\n",
    "    CASE \n",
    "        WHEN c.CEC7_R IS NULL THEN NULL\n",
    "        WHEN c.CEC7_R < 10 THEN 'Low CEC - Limited K holding'\n",
    "        WHEN c.CEC7_R < 25 THEN 'Medium CEC - Moderate K holding'\n",
    "        ELSE 'High CEC - Good K holding'\n",
    "    END as POTASSIUM_HOLDING_CAPACITY,\n",
    "    \n",
    "    -- Calcium availability\n",
    "    c.CACO3_R as CALCIUM_CARBONATE_PCT,\n",
    "    CASE \n",
    "        WHEN c.CACO3_R > 5 THEN 'High Ca - Calcareous'\n",
    "        WHEN c.SUMBASES_R > 80 THEN 'Adequate Ca'\n",
    "        WHEN c.SUMBASES_R > 50 THEN 'Moderate Ca'\n",
    "        ELSE 'Potentially Low Ca'\n",
    "    END as CALCIUM_STATUS,\n",
    "    \n",
    "    -- Sulfur indicators\n",
    "    c.GYPSUM_R as GYPSUM_PCT,\n",
    "    CASE \n",
    "        WHEN c.GYPSUM_R > 1 THEN 'High S - Gypsum present'\n",
    "        WHEN c.OM_R > 2 THEN 'Moderate S - Organic matter'\n",
    "        ELSE 'Potentially Low S'\n",
    "    END as SULFUR_STATUS,\n",
    "    \n",
    "    -- Micronutrient availability\n",
    "    CASE \n",
    "        WHEN c.PH1TO1H2O_R > 7.5 THEN 'Iron/Zinc/Manganese may be deficient'\n",
    "        WHEN c.PH1TO1H2O_R < 5.5 THEN 'Aluminum toxicity possible'\n",
    "        ELSE 'Micronutrients generally available'\n",
    "    END as MICRONUTRIENT_AVAILABILITY,\n",
    "    \n",
    "    -- Salinity impact on nutrients\n",
    "    c.EC_R as ELECTRICAL_CONDUCTIVITY,\n",
    "    CASE \n",
    "        WHEN c.EC_R > 4 THEN 'High salinity - Nutrient uptake impaired'\n",
    "        WHEN c.EC_R > 2 THEN 'Moderate salinity - Monitor nutrient uptake'\n",
    "        ELSE 'Normal salinity'\n",
    "    END as SALINITY_IMPACT\n",
    "\n",
    "FROM SSURGO_CHORIZON c\n",
    "WHERE c.HZDEPT_R < 30  -- Focus on top 30cm where most roots are\n",
    "ORDER BY c.COKEY, c.HZDEPT_R;\n",
    "\n",
    "-- CROP-SPECIFIC NUTRIENT RECOMMENDATIONS VIEW\n",
    "CREATE OR REPLACE VIEW CROP_NUTRIENT_NEEDS AS\n",
    "SELECT \n",
    "    sn.*,\n",
    "    comp.COMPNAME,\n",
    "    comp.MUKEY,\n",
    "    mu.MUNAME,\n",
    "    \n",
    "    -- Corn nutrient recommendations\n",
    "    CASE \n",
    "        WHEN sn.PHOSPHORUS_LEVEL IN ('Very Low', 'Low') THEN 'Apply 60-80 lbs P2O5/acre'\n",
    "        WHEN sn.PHOSPHORUS_LEVEL = 'Medium' THEN 'Apply 40-60 lbs P2O5/acre'\n",
    "        WHEN sn.PHOSPHORUS_LEVEL = 'High' THEN 'Apply 20-30 lbs P2O5/acre'\n",
    "        ELSE 'Maintenance only'\n",
    "    END as CORN_PHOSPHORUS_REC,\n",
    "    \n",
    "    CASE \n",
    "        WHEN sn.NITROGEN_POTENTIAL IN ('Very Low', 'Low') THEN 'Apply 180-220 lbs N/acre'\n",
    "        WHEN sn.NITROGEN_POTENTIAL = 'Medium' THEN 'Apply 150-180 lbs N/acre'\n",
    "        WHEN sn.NITROGEN_POTENTIAL = 'High' THEN 'Apply 120-150 lbs N/acre'\n",
    "        ELSE 'Apply 100-120 lbs N/acre'\n",
    "    END as CORN_NITROGEN_REC,\n",
    "    \n",
    "    -- Soybean nutrient recommendations\n",
    "    CASE \n",
    "        WHEN sn.PHOSPHORUS_LEVEL IN ('Very Low', 'Low') THEN 'Apply 40-60 lbs P2O5/acre'\n",
    "        WHEN sn.PHOSPHORUS_LEVEL = 'Medium' THEN 'Apply 20-40 lbs P2O5/acre'\n",
    "        ELSE 'No P needed'\n",
    "    END as SOYBEAN_PHOSPHORUS_REC,\n",
    "    \n",
    "    -- Lime recommendations\n",
    "    CASE \n",
    "        WHEN sn.SOIL_PH < 5.5 THEN 'Apply 2-3 tons lime/acre'\n",
    "        WHEN sn.SOIL_PH < 6.0 THEN 'Apply 1-2 tons lime/acre'\n",
    "        WHEN sn.SOIL_PH < 6.5 THEN 'Apply 0.5-1 ton lime/acre'\n",
    "        ELSE 'No lime needed'\n",
    "    END as LIME_RECOMMENDATION,\n",
    "    \n",
    "    -- Fertilizer cost estimates (per acre)\n",
    "    CASE \n",
    "        WHEN sn.PHOSPHORUS_LEVEL IN ('Very Low', 'Low') THEN 180\n",
    "        WHEN sn.PHOSPHORUS_LEVEL = 'Medium' THEN 120\n",
    "        WHEN sn.PHOSPHORUS_LEVEL = 'High' THEN 60\n",
    "        ELSE 0\n",
    "    END as ESTIMATED_P_COST_PER_ACRE,\n",
    "    \n",
    "    CASE \n",
    "        WHEN sn.NITROGEN_POTENTIAL IN ('Very Low', 'Low') THEN 220\n",
    "        WHEN sn.NITROGEN_POTENTIAL = 'Medium' THEN 180\n",
    "        WHEN sn.NITROGEN_POTENTIAL = 'High' THEN 140\n",
    "        ELSE 120\n",
    "    END as ESTIMATED_N_COST_PER_ACRE\n",
    "\n",
    "FROM SOIL_NUTRIENTS sn\n",
    "JOIN SSURGO_COMPONENT comp ON sn.COKEY = comp.COKEY\n",
    "JOIN SSURGO_MAPUNIT mu ON comp.MUKEY = mu.MUKEY\n",
    "WHERE comp.MAJCOMPFLAG = 'Yes';\n",
    "\n",
    "-- BUSINESS SUMMARY VIEW\n",
    "CREATE OR REPLACE VIEW SOIL_BUSINESS_SUMMARY AS\n",
    "SELECT \n",
    "    m.AREASYMBOL,\n",
    "    m.MUSYM,\n",
    "    mu.MUNAME,\n",
    "    c.COMPNAME,\n",
    "    c.COMPPCT_R,\n",
    "    c.DRAINAGECL,\n",
    "    c.HYDRICRATING,\n",
    "    c.SLOPE_R,\n",
    "    c.HYDGRP,\n",
    "    sn.SOIL_PH,\n",
    "    sn.PH_CATEGORY,\n",
    "    sn.PHOSPHORUS_LEVEL,\n",
    "    sn.NITROGEN_POTENTIAL,\n",
    "    sn.ORGANIC_MATTER_PCT,\n",
    "    sn.MICRONUTRIENT_AVAILABILITY,\n",
    "    sn.SALINITY_IMPACT,\n",
    "    cn.CORN_PHOSPHORUS_REC,\n",
    "    cn.CORN_NITROGEN_REC,\n",
    "    cn.LIME_RECOMMENDATION,\n",
    "    cn.ESTIMATED_P_COST_PER_ACRE,\n",
    "    cn.ESTIMATED_N_COST_PER_ACRE,\n",
    "    (cn.ESTIMATED_P_COST_PER_ACRE + cn.ESTIMATED_N_COST_PER_ACRE) as TOTAL_FERTILIZER_COST_PER_ACRE,\n",
    "    m.GEOMETRY\n",
    "FROM SSURGO_MUPOLYGON m\n",
    "JOIN SSURGO_MAPUNIT mu ON m.MUKEY = mu.MUKEY\n",
    "JOIN SSURGO_COMPONENT c ON mu.MUKEY = c.MUKEY\n",
    "LEFT JOIN CROP_NUTRIENT_NEEDS cn ON c.COKEY = cn.COKEY\n",
    "LEFT JOIN SOIL_NUTRIENTS sn ON c.COKEY = sn.COKEY\n",
    "WHERE c.MAJCOMPFLAG = 'Yes'\n",
    "  AND (sn.HZDEPT_R IS NULL OR sn.HZDEPT_R = (\n",
    "      SELECT MIN(HZDEPT_R) FROM SSURGO_CHORIZON WHERE COKEY = c.COKEY\n",
    "  ))\n",
    "ORDER BY m.AREASYMBOL, m.MUSYM, c.COMPPCT_R DESC;"
   ]
