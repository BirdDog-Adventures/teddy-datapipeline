    "# PYTHON CELL 7: Nutrient Analysis Dashboard\n",
    "st.subheader(\"Nutrient Analysis Dashboard\")\n",
    "\n",
    "analysis_type = st.selectbox(\n",
    "    \"Select Analysis Type\",\n",
    "    [\"pH Distribution\", \"Phosphorus Levels\", \"Nitrogen Potential\", \"Fertilizer Cost Analysis\", \"Problem Soils\"]\n",
    ")\n",
    "\n",
    "if st.button(\"Run Nutrient Analysis\"):\n",
    "    try:\n",
    "        if analysis_type == \"pH Distribution\":\n",
    "            ph_analysis = session.sql(\"\"\"\n",
    "            SELECT \n",
    "                PH_CATEGORY,\n",
    "                COUNT(*) as soil_count,\n",
    "                ROUND(AVG(SOIL_PH), 2) as avg_ph,\n",
    "                MIN(SOIL_PH) as min_ph,\n",
    "                MAX(SOIL_PH) as max_ph\n",
    "            FROM SOIL_NUTRIENTS\n",
    "            WHERE SOIL_PH IS NOT NULL\n",
    "            GROUP BY PH_CATEGORY\n",
    "            ORDER BY avg_ph\n",
    "            \"\"\").to_pandas()\n",
    "            \n",
    "            st.write(\"**Texas Soil pH Distribution:**\")\n",
    "            st.dataframe(ph_analysis)\n",
    "            \n",
    "        elif analysis_type == \"Phosphorus Levels\":\n",
    "            p_analysis = session.sql(\"\"\"\n",
    "            SELECT \n",
    "                PHOSPHORUS_LEVEL,\n",
    "                COUNT(*) as soil_count,\n",
    "                ROUND(AVG(PHOSPHORUS_BRAY1_PPM), 1) as avg_p_ppm\n",
    "            FROM SOIL_NUTRIENTS\n",
    "            WHERE PHOSPHORUS_BRAY1_PPM IS NOT NULL\n",
    "            GROUP BY PHOSPHORUS_LEVEL\n",
    "            ORDER BY \n",
    "                CASE PHOSPHORUS_LEVEL\n",
    "                    WHEN 'Very Low' THEN 1\n",
    "                    WHEN 'Low' THEN 2  \n",
    "                    WHEN 'Medium' THEN 3\n",
    "                    WHEN 'High' THEN 4\n",
    "                    WHEN 'Very High' THEN 5\n",
    "                    ELSE 6\n",
    "                END\n",
    "            \"\"\").to_pandas()\n",
    "            \n",
    "            st.write(\"**Texas Soil Phosphorus Levels:**\")\n",
    "            st.dataframe(p_analysis)\n",
    "            \n",
    "        elif analysis_type == \"Nitrogen Potential\":\n",
    "            n_analysis = session.sql(\"\"\"\n",
    "            SELECT \n",
    "                NITROGEN_POTENTIAL,\n",
    "                COUNT(*) as soil_count,\n",
    "                ROUND(AVG(ORGANIC_MATTER_PCT), 2) as avg_om_pct,\n",
    "                ROUND(AVG(ESTIMATED_NITROGEN_PPM), 0) as avg_n_ppm\n",
    "            FROM SOIL_NUTRIENTS\n",
    "            WHERE ORGANIC_MATTER_PCT IS NOT NULL\n",
    "            GROUP BY NITROGEN_POTENTIAL\n",
    "            ORDER BY avg_om_pct\n",
    "            \"\"\").to_pandas()\n",
    "            \n",
    "            st.write(\"**Texas Soil Nitrogen Potential:**\")\n",
    "            st.dataframe(n_analysis)\n",
    "            \n",
    "        elif analysis_type == \"Fertilizer Cost Analysis\":\n",
    "            cost_analysis = session.sql(\"\"\"\n",
    "            SELECT \n",
    "                PHOSPHORUS_LEVEL,\n",
    "                NITROGEN_POTENTIAL,\n",
    "                COUNT(*) as area_count,\n",
    "                AVG(ESTIMATED_P_COST_PER_ACRE) as avg_p_cost,\n",
    "                AVG(ESTIMATED_N_COST_PER_ACRE) as avg_n_cost,\n",
