    "                END as AG_QUALITY_RATING\n",
    "            FROM SOIL_BUSINESS_SUMMARY\n",
    "            WHERE ORGANIC_MATTER_PCT IS NOT NULL\n",
    "              AND DRAINAGECL IN ('Well drained', 'Moderately well drained')\n",
    "            GROUP BY AREASYMBOL, COMPNAME, PH_CATEGORY, PHOSPHORUS_LEVEL\n",
